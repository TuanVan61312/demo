<table class="table-bordered bg-light mt-3 mb-3 text-center">
    <tr>
        <th class="text-primary">Student list</th>
    </tr>
    <tr>
        <th>Name</th>
        <th>Email</th>
        <th>gender</th>
        <th>Date</th>
        <th>Image</th>
        <th>Menu</th>
    </tr>
    {{#each student}}
    <tr>
        <th>{{name}}</th>
        <th>{{email}}</th>
        <th>{{gender}}</th>
        <th>{{dateFormat dob "DD/MM/YYYY"}}</th>
        <th><img src="{{image}}" width="100" height="100"/></th>
        <th>
            <a class="btn btn-warning" href="student/edit">Edit</a>
            <a class="btn btn-warning" href="student/edit">Delete</a>
        </th>
    </tr>
    {{/each}}
